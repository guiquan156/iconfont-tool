{
  "private": true,
  "author": {
    "name": "chencheng",
    "email": "sorrycc@gmail.com"
  },
  "build": {
    "files": [
      "./dist/**/*",
      "./pages/**/*",
      "./node_modules/**/*"
    ],
    "appId": "com.sorrycc.umi-example-electron",
    "asar": false
  },
  "scripts": {
    "start": "electron app/dist/main/main",
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:main\"",
    "dev:main": "webpack --config webpack/webpack.main.config.js",
    "dev:renderer": "webpack --config webpack/webpack.renderer.config.js",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "",
    "build:renderer": "",
    "pack": "npm run build && npm run rebuild && build",
    "pack:dir": "npm run build && npm run rebuild && build --dir",
    "pack:dirOnly": "build --dir",
    "rebuild": "electron-rebuild -m ./app"
  },
  "devDependencies": {
    "@babel/runtime": "^7.0.0-beta.49",
    "concurrently": "^3.5.1",
    "electron": "^2.0.2",
    "electron-builder": "^20.15.1",
    "electron-debug": "^2.0.0",
    "electron-rebuild": "^1.7.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  }
}
